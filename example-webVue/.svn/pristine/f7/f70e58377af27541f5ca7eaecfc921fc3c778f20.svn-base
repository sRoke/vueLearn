<template>
  <div>
    <el-row>
      <div class="m-b-20 ovf-hd">
        <div class="fr w-800 m-l-30">
          <el-row type='flex'  :gutter='20'>
            <el-col :span='6' :offset="1">
            <el-select v-model="coo_model" placeholder="合作模式" @change="select()">
                <el-option
                  v-for="item in coo_model1"
                  :label="item.name"
                  :value="item.values">
                </el-option>
            </el-select>
            </el-col>
          <div class="block fr m-r-5">
        <!-- <el-date-picker
          v-model="time"
          type="daterange"
          placeholder="选择日期范围"
          format="yyyy-MM-dd"
          @change="timeSearch()">
        </el-date-picker> -->
          <el-select v-model="status" placeholder="请选择需求状态" @change="select()">
            <el-option
              v-for="item in statusList"
              :label="item.name"
              :value="item.status">
            </el-option>
          </el-select>
      </div>
            <el-col :span='10 '>
              <el-input placeholder="请输入需求名称、服务方名称" v-model="keywords">
                <el-button slot="append" icon="search" @click="select()"></el-button>
              </el-input>
            </el-col>
            <reset-btn class="fr m-l-5 m-r-10"
                 :reset="handleClearFilter"></reset-btn>
          </el-row>
      </div>
    </div>
    </el-row>
    <el-table
        :data="tableData"
        style="width: 100%"
        v-loading.fullscreen.lock="fullscreenLoading" element-loading-text="正在加载中"
        @row-click="openEdit">
        <el-table-column
          prop="number"
          label="需求编号"
          show-overflow-tooltip>
        </el-table-column>
        <el-table-column
          prop="title"
          label="需求标题"
          show-overflow-tooltip>
        </el-table-column>
        <el-table-column
          prop="coo_model"
          label="合作模式"
          show-overflow-tooltip>
        </el-table-column>
        <el-table-column
          prop="total_amount"
          label="项目金额"
          show-overflow-tooltip>
        </el-table-column>
        <el-table-column
          prop="total_amount"
          label="提成金额"
          show-overflow-tooltip>
        </el-table-column>
        <el-table-column
          prop="servicer"
          label="服务方"
          show-overflow-tooltip>
        </el-table-column>
        <el-table-column
          prop="create_time"
          label="创建日期"
          show-overflow-tooltip>
        </el-table-column>
        <el-table-column
          prop="status_name"
          label="状态">
        </el-table-column>
    </el-table>

    <div class="pos-rel p-t-20 h-50" v-show="tableData.length > 0">
      <div class="block pages m-b-10">
        <el-pagination
        @current-change="handleCurrentChange"
        layout="prev, pager, next, jumper"
        :page-size="pageSize"
        :current-page="currentPage"
        :total="dataCount">
        </el-pagination>
      </div>
    </div>

  </div>
</template>
<style type="text/css">
  .page-part{
    margin-top: 10px;
  }
</style>

<script>
  import http from '../../../assets/js/http'
  import resetBtn from '../../common/reset.vue'
  export default{
    data() {
      return {
        fullscreenLoading: false,
        time: '',
        currentPage: 1,
        dataCount: 1,
        keywords: '',
        pageSize: 20,
        tableData: [],
        statusList: [],
        status: '',
        coo_model: null,
        order_value: '',
        cus_id: null,
        startT: '',
        endT: '',
        coo_model1: [{
          name: '项目制',
          values: 1
        }, {
          name: '销量提成',
          values: 2
        }]
      }
    },
    watch: {
      '$route' (to, from) {             // 监听路由改变
        var self = this
        self.init()
      }
    },
    methods: {
      handleClearFilter() {
        this.keywords = ''
        // this.time = []
        // this.startT = ''
        // this.endT = ''
        this.coo_model = null
        this.status = ''
        this.select()
      },
      select() { // 通过路由的参数保存页面的状态
        var self = this
        console.log(self.coo_model)
        router.push({ params: { id: self.cus_id }, query: { ...this.$route.query, page: self.currentPage, keyword: self.keywords, status: self.status, coo_model: self.coo_model }})
      },
      init() { // 根据页面状态获取需求记录列表
        this.fullscreenLoading = true
        var self = this
        self.getStatus()
      },
      getQuery() { // 对路由的参数进行判断
        var self = this
        if (self.$route.query) {
          if (self.$route.query.keyword) {
            self.keywords = self.$route.query.keyword
          } else {
            self.keywords = ''
          }
          if (self.$route.query.page) {
            self.currentPage = self.$route.query.page * 1
          } else {
            self.currentPage = 1
          }
          // if (self.$route.query.start_time && self.$route.query.end_time) {
          //   self.time = []
          //   self.startT = self.$route.query.start_time
          //   self.endT = self.$route.query.end_time
          //   self.time.push(moment(self.startT, moment.ISO_8601)._d)
          //   self.time.push(moment(self.endT, moment.ISO_8601)._d)
          // } else {
          //   self.time = ''
          // }
          if (self.$route.query.status) {
            self.status = Number(self.$route.query.status)
          } else if (self.$route.query.status == 0) {
            self.status = self.$route.query.status
          } else {
            self.status = ''
          }
        }
        self.getData()
      },
      // timeSearch() { // 需求记录的时间范围的刷选
      //   var self = this
      //   if (self.time[0] != null) {
      //     self.startT = moment(self.time[0]).format('YYYY-MM-DD')
      //     self.endT = moment(self.time[1]).format('YYYY-MM-DD')
      //     self.select()
      //   } else {
      //     self.time = ''
      //     self.startT = ''
      //     self.endT = ''
      //     self.select()
      //   }
      // },
      getData() { // 获取需求记录列表
        this.fullscreenLoading = true
        var self = this
        let data = {
          params: {
            keyword: self.keywords,
            page: self.currentPage,
            pagesize: self.pageSize,
            status: self.status,
            coo_model: self.coo_model
          }
        }
        self.apiGet('demand_user/' + self.$route.query.dsu_id + '/requires', data).then((res) => {
          // console.log('restttttttttttt=' + _g.j2s(res))
          this.fullscreenLoading = false
          if (res.code == 200) {
            console.log(res.data)
            self.tableData = res.data.list
            self.currentPage = res.data.current_page
            self.pageSize = parseInt(res.data.page_size)
            self.dataCount = res.data.total_pages * self.pageSize
            // console.log(self.dataCount)
          } else {
            _g.dealError(self, res)
          }
        })
      },
      getStatus() { // 获取需求状态
        var self = this
        self.apiGet('requires/status_list?type=0').then((res) => {
          // console.log('getStatus=' + _g.j2s(res))
          if (res.code == 200) {
            self.statusList = res.data
            // console.log(self.statusList)
            self.statusList.unshift({ name: '全部', status: 0 })
            self.getQuery()
          } else {
            _g.dealError(self, res)
          }
        })
      },
      openEdit(row) { // 查看需求记录
        // console.log(row)
        location.hash = '/data/service/demanderDetail/' + row.id
      },
      handleSizeChange(val) {
        // console.log(`每页 ${val} 条`)
      },
      handleCurrentChange(val) { // 分页的选择
        var self = this
        self.currentPage = val
        self.getData()
      }
    },
    created() {
      var self = this
      self.cus_id = self.$route.params.id
    },
    components: {
      'reset-btn': resetBtn
    },
    mixins: [http]
}
</script>